{
  "manifest_version": 2,
  "name": "Twitter Image Viewer",
  "version": "1.2",
  "icons": {
    "16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
  },
  "description": "タイムラインからテキストを非表示することで画像閲覧を円滑に（したい）。",
  "permissions": ["https://twitter.com/*"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}

